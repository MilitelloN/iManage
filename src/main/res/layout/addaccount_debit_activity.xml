<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="30dp"
        android:text="iManage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre de la cuenta"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="108dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripcion"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cuenta debito"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="233dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cuenta credito"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="233dp" />

    <Switch
        android:id="@+id/sw_accType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/txt_nameAcc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="96dp" />

    <EditText
        android:id="@+id/txt_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="151dp" />

    <Button
        android:id="@+id/bttn_AddAccount"
        android:layout_width="167dp"
        android:layout_height="97dp"
        android:text="Agregar cuenta"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="341dp" />

    <Button
        android:id="@+id/bttn_cancel"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:text="Cancelar"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="446dp" />
</androidx.constraintlayout.widget.ConstraintLayout>